<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="index" namespace="/" extends="appstore-default">
        <action name="*" method="{1}" class="com.jd.appstore.web.action.cp.IndexAction">
            <result name="success">/WEB-INF/vm/web/{1}.vm</result>
            <result name="index" type="redirect">/index.action</result>
        </action>
    </package>

</struts>
